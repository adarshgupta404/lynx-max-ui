{
  "name": "lynx-max-ui",
  "version": "0.1.2",
  "type": "module",
  "description": "Lynx component library + CLI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rspeedy build",
    "dev": "rspeedy dev",
    "preview": "rspeedy preview",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist && rspeedy build --clearCache"
  },
  "bin": {
    "lynx-max-ui": "./bin/cli.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@lynx-js/react": "^0.105.2",
    "clsx": "^2.1.1",
    "prompts": "^2.4.2",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@lynx-js/qrcode-rsbuild-plugin": "^0.3.4",
    "@lynx-js/react-rsbuild-plugin": "^0.9.2",
    "@lynx-js/rspeedy": "^0.8.4",
    "@lynx-js/tailwind-preset": "^0.0.2",
    "@lynx-js/types": "^3.2.0",
    "@types/react": "^18.3.18",
    "rsbuild-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "3",
    "typescript": "~5.7.3"
  },
  "engines": {
    "node": ">=18"
  }
}
